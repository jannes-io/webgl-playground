{
  "extends": [
    "plugin:import/recommended",
    "airbnb-typescript"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "project": "./tsconfig.json"
  },
  "rules": {
    "spaced-comment": 0,
    "no-prototype-builtins": 0,
    "object-curly-newline": 0,
    "import/no-cycle": 0,
    "import/no-named-as-default": 0,
    "react/jsx-filename-extension": 0,
    "curly": ["error", "all"]
  }
}
